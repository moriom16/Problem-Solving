#include<iostream>
using namespace std;
int main(){
//n= given number and k= number of steps 
    int n,k;
    cin>>n>>k;

//if last digit of n is 0 it'll be mudules by 10. n= 520 % 10 =>52
//if last digit of n is not 0 then decrease the number by 1
//continue the process for k steps

    for(int i=0; i<k; i++){
        if(n%10==0){
            n/10;
        }
        else
            n--;
    }cout<<n<<endl;
}
