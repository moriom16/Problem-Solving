#include <bits/stdc++.h>
 
using namespace std;
 
int main() {
    int n,x=0,y=0;
    cin >> n;
    string s;
    cin>>s;
    for(int i=0; i<n; i++){
        if(s[i]=='A'){
            x++;
        }
        else    y++;
    }
    if(x>y){
        cout<<"Anton"<<endl;
    }
    else if(y>x){
        cout<<"Danik"<<endl;
    }
    else
        cout<<"Friendship"<<endl;
 
    return 0;
}
